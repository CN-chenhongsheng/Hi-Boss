{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^qiun-ucharts$": "@/components/qiun-ucharts/index.vue"
    }
  },
  "pages": [
    {
      "path": "pages/tab/home/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/apply/index",
      "style": {
        "navigationBarTitleText": "申请中心",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/profile/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages/common",
      "pages": [
        {
          "path": "login/index",
          "style": {
            "navigationStyle": "custom"
          }
        },
        {
          "path": "webview/index",
          "style": {
            "navigationBarTitleText": "网页"
          }
        },
        {
          "path": "dorm-info/index",
          "style": {
            "navigationBarTitleText": "宿舍信息"
          }
        }
      ]
    },
    {
      "root": "pages/profile",
      "pages": [
        {
          "path": "edit/index",
          "style": {
            "navigationStyle": "custom"
          }
        },
        {
          "path": "change-password/index",
          "style": {
            "navigationStyle": "custom"
          }
        },
        {
          "path": "student-habits/index",
          "style": {
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pages/apply",
      "pages": [
        {
          "path": "detail/index",
          "style": {
            "navigationBarTitleText": "申请详情",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "form/index",
          "style": {
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pages/service",
      "pages": [
        {
          "path": "repair-list/index",
          "style": {
            "navigationBarTitleText": "报修记录"
          }
        },
        {
          "path": "repair-detail/index",
          "style": {
            "navigationBarTitleText": "报修详情"
          }
        },
        {
          "path": "notice-detail/index",
          "style": {
            "navigationBarTitleText": "公告详情"
          }
        },
        {
          "path": "message/index",
          "style": {
            "navigationBarTitleText": "消息中心",
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pages/allocation",
      "pages": [
        {
          "path": "survey/index",
          "style": {
            "navigationBarTitleText": "生活习惯问卷",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "result/index",
          "style": {
            "navigationBarTitleText": "分配结果",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "recommend/index",
          "style": {
            "navigationBarTitleText": "床位推荐",
            "navigationStyle": "custom"
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "pages/tab/home/index": {
      "network": "all",
      "packages": ["pages/common"]
    }
  },
  "tabBar": {
    "color": "#8E8E93",
    "selectedColor": "#0adbc3",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [{
      "pagePath": "pages/tab/home/index",
      "text": "首页",
      "iconPath": "static/images/tabbar/home.png",
      "selectedIconPath": "static/images/tabbar/home-active.png"
    }, {
      "pagePath": "pages/tab/apply/index",
      "text": "申请",
      "iconPath": "static/images/tabbar/apply.png",
      "selectedIconPath": "static/images/tabbar/apply-active.png"
    }, {
      "pagePath": "pages/tab/profile/index",
      "text": "我的",
      "iconPath": "static/images/tabbar/profile.png",
      "selectedIconPath": "static/images/tabbar/profile-active.png"
    }]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
