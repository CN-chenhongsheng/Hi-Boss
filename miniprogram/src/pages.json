{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^qiun-ucharts$": "@/components/qiun-ucharts/index.vue"
    }
  },
  "pages": [
    {
      "path": "pages/tab/home/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/apply/index",
      "style": {
        "navigationBarTitleText": "申请中心",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/statistics/index",
      "style": {
        "navigationBarTitleText": "数据统计"
      }
    },
    {
      "path": "pages/tab/message/index",
      "style": {
        "navigationBarTitleText": "消息中心",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/profile/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages/common",
      "pages": [
        {
          "path": "login/index",
          "style": {
            "navigationStyle": "custom"
          }
        },
        {
          "path": "webview/index",
          "style": {
            "navigationBarTitleText": "网页"
          }
        },
        {
          "path": "dorm-info/index",
          "style": {
            "navigationBarTitleText": "宿舍信息"
          }
        }
      ]
    },
    {
      "root": "pages/profile",
      "pages": [
        {
          "path": "lifestyle/index",
          "style": {
            "navigationBarTitleText": "生活习惯",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "student-habits/index",
          "style": {
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pages/apply",
      "pages": [
        {
          "path": "check-in/index",
          "style": {
            "navigationBarTitleText": "入住申请"
          }
        },
        {
          "path": "transfer/index",
          "style": {
            "navigationBarTitleText": "调宿申请"
          }
        },
        {
          "path": "check-out/index",
          "style": {
            "navigationBarTitleText": "退宿申请"
          }
        },
        {
          "path": "stay/index",
          "style": {
            "navigationBarTitleText": "留宿申请"
          }
        },
        {
          "path": "detail/index",
          "style": {
            "navigationBarTitleText": "申请详情",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "form/index",
          "style": {
            "navigationBarTitleText": "业务填报",
            "navigationStyle": "custom"
          }
        }
      ]
    },
    {
      "root": "pages/service",
      "pages": [
        {
          "path": "repair/index",
          "style": {
            "navigationBarTitleText": "报修申请"
          }
        },
        {
          "path": "repair-list/index",
          "style": {
            "navigationBarTitleText": "报修记录"
          }
        },
        {
          "path": "notice-detail/index",
          "style": {
            "navigationBarTitleText": "公告详情"
          }
        }
      ]
    },
    {
      "root": "pages/admin",
      "pages": [
        {
          "path": "approval-detail/index",
          "style": {
            "navigationBarTitleText": "审批详情"
          }
        },
        {
          "path": "statistics/index",
          "style": {
            "navigationBarTitleText": "数据统计"
          }
        },
        {
          "path": "notice-manage/index",
          "style": {
            "navigationBarTitleText": "公告管理"
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "pages/tab/home/index": {
      "network": "all",
      "packages": ["pages/common"]
    }
  },
  "tabBar": {
    "color": "#8E8E93",
    "selectedColor": "#0adbc3",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [{
      "pagePath": "pages/tab/home/index",
      "text": "首页",
      "iconPath": "static/images/tabbar/home.png",
      "selectedIconPath": "static/images/tabbar/home-active.png"
    }, {
      "pagePath": "pages/tab/apply/index",
      "text": "申请",
      "iconPath": "static/images/tabbar/apply.png",
      "selectedIconPath": "static/images/tabbar/apply-active.png"
    }, {
      "pagePath": "pages/tab/profile/index",
      "text": "我的",
      "iconPath": "static/images/tabbar/profile.png",
      "selectedIconPath": "static/images/tabbar/profile-active.png"
    }]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
